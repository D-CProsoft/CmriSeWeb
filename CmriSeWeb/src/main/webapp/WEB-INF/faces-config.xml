<?xml version="1.0"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xi="http://www.w3.org/2001/XInclude" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
                                   http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <application>
    <!-- Dialog Framework -->
	<action-listener>org.primefaces.application.DialogActionListener</action-listener>
	<navigation-handler>org.primefaces.application.DialogNavigationHandler</navigation-handler>
	<view-handler>org.primefaces.application.DialogViewHandler</view-handler>
	<locale-config><default-locale>es</default-locale>
    </locale-config>
    <!--etiquetas del formulario-->
    <resource-bundle>
    <!--No se necesita agregar la extension del archivo-->
    <base-name>mensajes</base-name>
    <var>msgs</var>
    </resource-bundle>
</application>

<navigation-rule>
        <navigation-case>
            <from-outcome>Admin-TablasUtilitariasvalores</from-outcome>
            <to-view-id>/cmrise/admin/TablasUtilitariasValores.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>PaginaPrincipal</from-outcome>
            <to-view-id>/cmrise/principal.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>Preguntas-ManageNewMrqs</from-outcome>
            <to-view-id>/cmrise/preguntas/mrqs/ManageNewMrqs.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
         <navigation-case>
            <from-outcome>Preguntas-CreateNewMrqs</from-outcome>
            <to-view-id>/cmrise/preguntas/mrqs/CreateMrq.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>Preguntas-UpdateNewMrqs</from-outcome>
            <to-view-id>/cmrise/preguntas/mrqs/UpdateMrq.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
         <navigation-case>
            <from-outcome>Preguntas-UpdateFreeTextAnswer-NewMrqs</from-outcome>
            <to-view-id>/cmrise/preguntas/mrqs/UpdateFreeTextAnswer.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
</navigation-rule>        
 
</faces-config>
